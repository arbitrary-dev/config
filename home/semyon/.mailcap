text/html; mv %s %s.html && qutebrowser %s.html &; test=test -n "$DISPLAY"; needsterminal;
text/html; w3m -I %{charset} -T text/html; copiousoutput;

application/msword; antiword '%s'; copiousoutput; description=Word Document; nametemplate=%s.doc
application/octet-stream; antiword '%s'; copiousoutput; description=Word Document; nametemplate=%s.doc

image/*; feh -g 640x480 --zoom fill %s &; test=test -n "$DISPLAY"; needsterminal;
application/pdf; mupdf %s &; test=test -n "$DISPLAY"; needsterminal; nametemplate=%s.pdf
